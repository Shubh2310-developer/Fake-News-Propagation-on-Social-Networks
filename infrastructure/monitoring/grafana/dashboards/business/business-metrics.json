{
  "dashboard": {
    "id": null,
    "title": "Business Metrics - Fake News Platform KPIs",
    "tags": ["business", "kpi", "fake-news", "ml", "game-theory"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 1,
    "hideControls": false,
    "links": [],
    "liveNow": false,
    "weekStart": "",
    "refresh": "1m",
    "schemaVersion": 37,
    "version": 1,
    "description": "Business and application-specific metrics for the Fake News Game Theory Platform including ML model performance, simulation analytics, and user engagement",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "templating": {
      "list": [
        {
          "current": {
            "selected": false,
            "text": "fakenews-production",
            "value": "fakenews-production"
          },
          "hide": 0,
          "includeAll": false,
          "multi": false,
          "name": "environment",
          "options": [],
          "query": "label_values(classifier_predictions_total, environment)",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 1,
          "type": "query",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        },
        {
          "current": {
            "selected": false,
            "text": "1h",
            "value": "1h"
          },
          "hide": 0,
          "includeAll": false,
          "multi": false,
          "name": "range",
          "options": [
            {"text": "5m", "value": "5m"},
            {"text": "15m", "value": "15m"},
            {"text": "1h", "value": "1h"},
            {"text": "6h", "value": "6h"},
            {"text": "24h", "value": "24h"}
          ],
          "query": "5m,15m,1h,6h,24h",
          "skipUrlSync": false,
          "type": "custom"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "enable": true,
          "expr": "increase(model_training_completed_total{environment=\"$environment\"}[1m]) > 0",
          "iconColor": "green",
          "name": "Model Training Events",
          "titleFormat": "Model Training: {{model_type}}",
          "useValueForTime": false
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "üìä Business KPIs Overview",
        "type": "stat",
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(increase(classifier_predictions_total{environment=\"$environment\"}[$range]))",
            "legendFormat": "Total Predictions",
            "refId": "A"
          },
          {
            "expr": "sum(increase(simulations_run_total{environment=\"$environment\"}[$range]))",
            "legendFormat": "Simulations Run",
            "refId": "B"
          },
          {
            "expr": "sum(increase(user_sessions_total{environment=\"$environment\"}[$range]))",
            "legendFormat": "User Sessions",
            "refId": "C"
          },
          {
            "expr": "avg(model_accuracy{environment=\"$environment\"}) * 100",
            "legendFormat": "Model Accuracy %",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Model Accuracy %"},
              "properties": [
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {"color": "red", "value": null},
                      {"color": "yellow", "value": 70},
                      {"color": "green", "value": 85}
                    ]
                  }
                },
                {"id": "unit", "value": "percent"}
              ]
            }
          ]
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 2,
        "title": "ü§ñ ML Model Performance",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
        "targets": [
          {
            "expr": "avg(model_accuracy{environment=\"$environment\"}) by (model_type) * 100",
            "legendFormat": "{{model_type}} - Accuracy %",
            "refId": "A"
          },
          {
            "expr": "avg(model_precision{environment=\"$environment\"}) by (model_type) * 100",
            "legendFormat": "{{model_type}} - Precision %",
            "refId": "B"
          },
          {
            "expr": "avg(model_recall{environment=\"$environment\"}) by (model_type) * 100",
            "legendFormat": "{{model_type}} - Recall %",
            "refId": "C"
          },
          {
            "expr": "avg(model_f1_score{environment=\"$environment\"}) by (model_type) * 100",
            "legendFormat": "{{model_type}} - F1 Score %",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "axisLabel": "Performance %",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {"legend": false, "tooltip": false, "viz": false},
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {"type": "linear"},
              "showPoints": "auto",
              "spanNulls": false,
              "stacking": {"group": "A", "mode": "none"},
              "thresholdsStyle": {"mode": "line"}
            },
            "mappings": [],
            "max": 100,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "green", "value": 85}
              ]
            },
            "unit": "percent"
          }
        },
        "options": {
          "legend": {
            "calcs": ["lastNotNull", "max", "min"],
            "displayMode": "table",
            "placement": "bottom"
          },
          "tooltip": {"mode": "multi", "sort": "none"}
        }
      },
      {
        "id": 3,
        "title": "üéØ Classifier Predictions Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
        "targets": [
          {
            "expr": "sum(increase(classifier_predictions_total{environment=\"$environment\",prediction=\"fake\"}[$range]))",
            "legendFormat": "Fake News Detected",
            "refId": "A"
          },
          {
            "expr": "sum(increase(classifier_predictions_total{environment=\"$environment\",prediction=\"real\"}[$range]))",
            "legendFormat": "Real News Detected",
            "refId": "B"
          },
          {
            "expr": "sum(increase(classifier_predictions_total{environment=\"$environment\",prediction=\"uncertain\"}[$range]))",
            "legendFormat": "Uncertain Classification",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "hideFrom": {"legend": false, "tooltip": false, "viz": false}
            },
            "mappings": [],
            "unit": "short"
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Fake News Detected"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}]
            },
            {
              "matcher": {"id": "byName", "options": "Real News Detected"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}]
            },
            {
              "matcher": {"id": "byName", "options": "Uncertain Classification"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "yellow"}}]
            }
          ]
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "pie",
          "tooltip": {"mode": "single", "sort": "none"},
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "values": ["percent"]
          }
        }
      },
      {
