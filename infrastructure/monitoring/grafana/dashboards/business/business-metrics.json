        "type": "stat",
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 30},
        "targets": [
          {
            "expr": "sum(increase(classifier_predictions_total{environment=\"$environment\",prediction=\"fake\"}[$range])) / sum(increase(classifier_predictions_total{environment=\"$environment\"}[$range])) * 100",
